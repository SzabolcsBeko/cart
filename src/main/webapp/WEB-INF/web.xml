<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>CartManagement</display-name>
  <welcome-file-list>
    <welcome-file>CartWebController</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>CartWebController</servlet-name>
  	<servlet-class>hu.webtown.cart.webcontroller.CartWebController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CartWebController</servlet-name>
  	<url-pattern>/cart/*</url-pattern>
  </servlet-mapping>
</web-app>